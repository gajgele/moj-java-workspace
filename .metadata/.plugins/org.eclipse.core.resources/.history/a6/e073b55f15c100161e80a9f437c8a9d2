
public class App {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int value = 0;
		
		while(value < 100){
			System.out.println("Iteracija broj: " + value);
			value++;
			
		}
		int value2 = 30;
		
		while(value2 < 50){
			System.out.println("Iteracija broj: " + value2);
			value2++;
			
		}
		int value3 = 50;
		
		while(value3 > 30){
			System.out.println("Iteracija broj: " + value3);
			value3--;
			
		}

	}

}
